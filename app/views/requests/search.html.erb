<h1>Search Results</h1>
<hr>
<div class="requests">
  <% @requests.each do |request| %>
    <h2><%= link_to request.name, request_path(request) %></h2>
    <%= request.email %>
    <%= request.department %>
    <%= request.message %>
    Completed: <%= request.completed %>
    <hr>
  <% end %>
</div>
<%= paginate @requests %>
